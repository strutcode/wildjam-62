[sub_resource type="GDScript" id="GDScript_6xhvm"]
script/source = "extends Node2D

const Enemy = preload('res://enemy.tscn')

@onready var spawn_rect = $SpawnRect

func _process(delta):
	%FPS.text = 'FPS: %.1f' % Performance.get_monitor(Performance.TIME_FPS)
	%Enemies.text = 'Enemies: %d' % get_tree().get_nodes_in_group('enemy').size()
	%Points.text = 'XP: %d/%d' % [Game.xp, Game.nextLvl]

func spawn():
	if Time.get_ticks_msec() > 2000:
		if Performance.get_monitor(Performance.TIME_FPS) < 120:
			return

	var inst = Enemy.instantiate()
	add_child(inst)
	inst.position.x = randf_range(spawn_rect.position.x, spawn_rect.position.x + spawn_rect.size.x)
	inst.position.y = randf_range(spawn_rect.position.y, spawn_rect.position.y + spawn_rect.size.y)
"

